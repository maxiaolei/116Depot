<div class="depot_form">
	<%=form_tag do%>
	<fieldset>
		<legend>please Log In</legend>
		<div>
			<label for="name">Name:</label>
			<%=text_field_tag :name,params[:name]%>
		</div>
		<div>
			<label for="password">Password</label>
			<%=password_field_tag :password,params[:password]%>
		<div>
			<%=submit_tag "Login"%>
		</div>
		</div>
	</fieldset>
<%end%>
</div>